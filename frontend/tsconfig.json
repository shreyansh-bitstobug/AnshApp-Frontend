{
  "compilerOptions": {
    "baseUrl": ".",
    "paths": {
      "@/*": [
        "./src/*"
      ]
    }
  },
  "extends": "expo/tsconfig.base",
  "include": [
    "global.d.ts",
    "**/*.ts",
    "**/*.tsx",
    "nativewind-env.d.ts",
    "src/app/_layout.jsx",
    "src/app/index.jsx",
    ".expo/types/**/*.ts",
    "expo-env.d.ts"
  ]
}
